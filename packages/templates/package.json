{
    "name": "@crawlee/templates",
    "version": "3.0.0",
    "description": "Templates for the crawlee projects",
    "engines": {
        "node": ">=16.0.0"
    },
    "main": "./dist/index.js",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/index.mjs",
            "require": "./dist/index.js",
            "types": "./dist/index.d.ts"
        },
        "./package.json": "./package.json"
    },
    "keywords": [
        "apify",
        "headless",
        "chrome",
        "puppeteer",
        "crawler",
        "scraper"
    ],
    "author": {
        "name": "Apify",
        "email": "support@apify.com",
        "url": "https://apify.com"
    },
    "license": "Apache-2.0",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/apify/apify-ts"
    },
    "bugs": {
        "url": "https://github.com/apify/apify-ts/issues"
    },
    "homepage": "https://crawlee.dev",
    "scripts": {
        "build": "npm run clean && npm run compile && npm run copy",
        "clean": "rimraf ./dist",
        "compile": "tsc -p tsconfig.build.json && gen-esm-wrapper ./dist/index.js ./dist/index.mjs",
        "copy": "node ./scripts/copy-templates.mjs && ts-node -T ../../scripts/copy.ts"
    },
    "publishConfig": {
        "access": "public"
    },
    "dependencies": {
        "ansi-colors": "^4.1.3",
        "inquirer": "^9.0.0",
        "yargonaut": "^1.1.4",
        "yargs": "^17.5.1"
    }
}
